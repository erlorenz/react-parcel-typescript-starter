    "@babel/preset-typescript": "^7.12.1",
    "@types/node": "^14.14.6",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/react-router-dom": "^5.1.6",
    "@types/styled-components": "^5.1.4",
    "babel-plugin-styled-components": "^1.11.1",
    "parcel": "^2.0.0-nightly.440",
    "prettier": "2.0.5",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "dayjs": "^1.9.5",
    "react": "^17.0.1",
